<Page x:Class="BookingApp.WPF.Views.OwnerView.SuperOwner"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BookingApp.WPF.Views.OwnerView"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SuperOwner">

    <Grid Background="LightBlue">
        <Label Content="SuperOwner" HorizontalAlignment="Left" Margin="116,176,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Center" Margin="0,207,0,0" TextWrapping="Wrap" Text="{Binding SuperOwner}" VerticalAlignment="Top" Width="563"/>


    </Grid>
</Page>
